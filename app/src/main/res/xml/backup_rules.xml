<full-backup-content>
    <!-- https://developer.android.com/identity/data/autobackup#XMLSyntax -->
    <include domain="sharedpref"/>
    <include domain="database"/>
    <!-- file storage currently holds the cache only -->
    <include domain="root"/>
    <exclude domain="root" path="cache/"/>
    <include domain="external"/>
    <exclude domain="external" path="cache/"/>
</full-backup-content>